{
  "version": "2.0.0",
  "tasks": [
    { "label": "compose up", "type": "shell", "command": "docker compose -f examples/local/compose.yml up -d --remove-orphans" },
    { "label": "compose down", "type": "shell", "command": "docker compose -f examples/local/compose.yml down" },
    { "label": "compose restart traefik", "type": "shell", "command": "docker compose -f examples/local/compose.yml restart traefik" },
    { "label": "compose traefik logs", "type": "shell", "command": "docker compose -f examples/local/compose.yml logs -f traefik" },
    { "label": "go test", "type": "shell", "command": "go test ./...", "group": "test" },
    { "label": "test request", "type": "shell", "command": "curl -i -H 'Host: demo.localhost' http://localhost:8080/" }
  ]
}
